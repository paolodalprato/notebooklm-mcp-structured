{
  "name": "notebooklm-mcp-structured",
  "version": "1.0.0",
  "description": "Enhanced MCP server for NotebookLM with structured prompts and source fidelity controls",
  "type": "module",
  "bin": {
    "notebooklm-mcp-structured": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "postbuild": "node -e \"console.log('Build complete')\"",
    "watch": "tsc --watch",
    "dev": "tsx watch src/index.ts",
    "prepare": "npm run build",
    "test": "tsx src/index.ts",
    "start": "node dist/index.js"
  },
  "keywords": [
    "mcp",
    "notebooklm",
    "gemini",
    "ai",
    "claude",
    "structured-prompts",
    "source-fidelity"
  ],
  "author": "Paolo Dal Prato (modifications) / Gérôme Dexheimer (original)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paolodalprato/notebooklm-mcp-structured.git"
  },
  "homepage": "https://github.com/paolodalprato/notebooklm-mcp-structured#readme",
  "bugs": {
    "url": "https://github.com/paolodalprato/notebooklm-mcp-structured/issues"
  },
  "files": [
    "dist",
    "README.md",
    "CUSTOM_MODIFICATIONS.md",
    "LICENSE",
    "docs"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "dotenv": "^16.4.0",
    "env-paths": "^3.0.0",
    "globby": "^14.1.0",
    "patchright": "^1.48.2",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
